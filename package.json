{
  "name": "commercetools-customui",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "mc-start": "dotenv -- mc-scripts start",
    "start": "concurrently \"npm run mc-start\" \"npm run generate-typescript\"",
    "compile-html": "NODE_ENV=production dotenv -- mc-scripts compile-html",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development dotenv -- mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "extract-intl": "formatjs extract --format=$(pwd)/intl-formatter.js --out-file=$(pwd)/src/i18n/data/core.json 'src/**/!(*.spec).js'",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "update-versions": "bulk-update-versions --match '^@commercetools-frontend/(?!ui-kit)(.*)'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.gql,.graphql",
    "generate-schema": "graphql-codegen --config codegen-generate-schema.yml",
    "generate-typescript": "graphql-codegen --config codegen-generate-typescript.yml --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@commercetools-frontend/actions-global": "^20.3.0",
    "@commercetools-frontend/application-components": "^20.3.0",
    "@commercetools-frontend/application-shell": "^20.3.0",
    "@commercetools-frontend/application-shell-connectors": "^20.3.0",
    "@commercetools-frontend/assets": "^18.5.4",
    "@commercetools-frontend/constants": "^20.3.0",
    "@commercetools-frontend/i18n": "^20.3.0",
    "@commercetools-frontend/permissions": "^20.3.0",
    "@commercetools-frontend/sdk": "^20.3.0",
    "@commercetools-uikit/card": "^12.0.0",
    "@commercetools-uikit/checkbox-input": "^12.0.6",
    "@commercetools-uikit/data-table": "^12.0.4",
    "@commercetools-uikit/flat-button": "^12.0.4",
    "@commercetools-uikit/grid": "^12.0.0",
    "@commercetools-uikit/icons": "^12.0.0",
    "@commercetools-uikit/loading-spinner": "^12.0.0",
    "@commercetools-uikit/money-field": "^12.0.6",
    "@commercetools-uikit/money-input": "^12.0.6",
    "@commercetools-uikit/primary-button": "^12.0.5",
    "@commercetools-uikit/secondary-button": "^12.0.5",
    "@commercetools-uikit/spacings": "^12.0.0",
    "@commercetools-uikit/tag": "^12.0.3",
    "@commercetools-uikit/text": "^12.0.0",
    "@commercetools-uikit/text-input": "^12.0.6",
    "@reduxjs/toolkit": "^1.5.1",
    "bulk-update-versions": "^1.1.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.18.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "^20.3.0",
    "@commercetools-frontend/mc-dev-authentication": "^19.0.0",
    "@commercetools-frontend/mc-scripts": "^20.3.0",
    "@formatjs/cli": "^4.2.16",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-react-apollo": "^2.2.5",
    "@testing-library/react": "^11.2.7",
    "@types/react": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "concurrently": "^6.2.0",
    "dotenv-cli": "^4.0.0",
    "enzyme": "3.11.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql": "^15.5.0",
    "jest": "27.0.1",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  }
}
