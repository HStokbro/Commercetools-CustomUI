{
  "name": "commercetools-customui",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "mc-start": "dotenv -- mc-scripts start",
    "start": "concurrently \"npm run mc-start\" \"npm run generate-typescript\"",
    "compile-html": "NODE_ENV=production dotenv -- mc-scripts compile-html",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development dotenv -- mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "extract-intl": "formatjs extract --format=$(pwd)/intl-formatter.js --out-file=$(pwd)/src/i18n/data/core.json 'src/**/!(*.spec).js'",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "update-versions": "bulk-update-versions --match '^@commercetools-frontend/(?!ui-kit)(.*)'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.gql,.graphql",
    "generate-schema": "graphql-codegen --config codegen-generate-schema.yml",
    "generate-typescript": "graphql-codegen --config codegen-generate-typescript.yml --watch"
  },
  "dependencies": {
    "@apollo/client": "3.3.7",
    "@commercetools-frontend/actions-global": "18.2.2",
    "@commercetools-frontend/application-components": "^18.2.2",
    "@commercetools-frontend/application-shell": "18.2.2",
    "@commercetools-frontend/application-shell-connectors": "^18.2.2",
    "@commercetools-frontend/assets": "18.1.0",
    "@commercetools-frontend/constants": "^18.2.2",
    "@commercetools-frontend/i18n": "18.2.2",
    "@commercetools-frontend/permissions": "18.2.2",
    "@commercetools-frontend/sdk": "^18.2.2",
    "@commercetools-uikit/checkbox-input": "^10.44.3",
    "@commercetools-uikit/data-table": "^10.44.2",
    "@commercetools-uikit/flat-button": "^10.45.0",
    "@commercetools-uikit/icons": "^10.44.4",
    "@commercetools-uikit/loading-spinner": "^10.44.4",
    "@commercetools-uikit/primary-button": "^10.46.1",
    "@commercetools-uikit/secondary-button": "^10.46.1",
    "@commercetools-uikit/spacings": "^10.44.4",
    "@commercetools-uikit/text": "^10.45.0",
    "bulk-update-versions": "1.1.2",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intl": "5.12.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "18.2.1",
    "@commercetools-frontend/mc-dev-authentication": "18.1.0",
    "@commercetools-frontend/mc-scripts": "18.2.0",
    "@formatjs/cli": "3.0.5",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@testing-library/react": "11.2.3",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "concurrently": "^5.3.0",
    "dotenv-cli": "4.0.0",
    "enzyme": "3.11.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql": "^15.5.0",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
